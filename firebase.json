{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ {
      "source": "/preview",
      "function": "previewFunction"    
    },
    {
      "source": "/preview/**",
      "function": "previewFunction"    
    } ]
  }
}
